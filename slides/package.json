{
  "name": "slides",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "reveal-md slides.md --watch",
    "pdf": "yarn createDist && reveal-md slides.md --print ./dist/slides.pdf",
    "static": "yarn createDist && reveal-md slides.md --static ./dist/_site",
    "createDist": "mkdir -p dist"
  },
  "dependencies": {
    "jquery": "^3.4.0",
    "reveal-md": "^3.0.1"
  }
}
